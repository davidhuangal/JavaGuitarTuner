<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="homePagePane" prefHeight="400.0" prefWidth="500.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guitartuner.HomePageController">
    <stylesheets>
        <URL value="@homepage.css" />
    </stylesheets>
   <children>
      <Label fx:id="styleChooser" layoutX="166.0" layoutY="97.0" text="Choose Your Style:" />
      <ImageView fx:id="acousticImageView" fitHeight="133.0" fitWidth="150.0" layoutX="46.0" layoutY="174.0" opacity="0.75" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@acoustic.jpg" />
         </image></ImageView>
      <ImageView fx:id="electricImageView" fitHeight="133.0" fitWidth="150.0" layoutX="320.0" layoutY="174.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@electric.jpg" />
         </image></ImageView>
      <Button fx:id="acousticButton" layoutX="78.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleGoToAcoustic" text="Acoustic" />
      <Button fx:id="electricButton" layoutX="355.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleGoToElectric" text="Electric" />
      <HBox fx:id="titleBox" alignment="CENTER" prefHeight="56.0" prefWidth="502.0">
         <children>
            <Label fx:id="headerLabel" text="Guitar Tuner 1.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
